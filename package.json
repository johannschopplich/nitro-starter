{
  "private": true,
  "packageManager": "pnpm@7.30.0",
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "dev": "nitro dev",
    "build": "nitro build",
    "preview": "node .output/server/index.mjs",
    "test": "vitest",
    "test:run": "vitest --run --no-threads",
    "test:types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,ts,tsx,yml}\" --write --ignore-path .gitignore",
    "prepare": "simple-git-hooks && nitro prepare"
  },
  "lint-staged": {
    "**/*.ts": "eslint --cache --ignore-path .gitignore"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "h3-zod": "^0.4.0",
    "lint-staged": "^13.2.0",
    "nitropack": "^2.3.2",
    "prettier": "^2.8.6",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^5.0.2",
    "vitest": "^0.29.7",
    "zod": "^3.21.4"
  }
}
