{
  "private": true,
  "packageManager": "pnpm@7.27.1",
  "engines": {
    "node": "^18"
  },
  "scripts": {
    "dev": "nitro dev",
    "build": "nitro build",
    "preview": "node .output/server/index.mjs",
    "test": "vitest",
    "test:run": "vitest --run --no-threads",
    "test:types": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"**/*.{css,html,json,md,ts,tsx,yml}\" --write --ignore-path .gitignore",
    "prepare": "simple-git-hooks && nitro prepare"
  },
  "lint-staged": {
    "**/*.ts": "eslint --cache --ignore-path .gitignore"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "h3-zod": "^0.3.10",
    "lint-staged": "^13.1.2",
    "nitropack": "^2.2.2",
    "prettier": "^2.8.4",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5",
    "zod": "^3.20.6"
  }
}
