{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "paths": {
      "~/*": ["./*"],
      "#imports": ["./.nitro/types/nitro-imports"],
      "#internal/nitro": ["./node_modules/nitropack/dist/runtime/index"],
      "#internal/nitro/*": ["./node_modules/nitropack/dist/runtime/*"]
    },
    "resolveJsonModule": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true
  },
  "include": ["./.nitro/types/nitro.d.ts", "**/*"]
}
